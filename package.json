{
  "name": "fallguys-frida-modmenu",
  "description": "Android Fall Guys cheat using FRIDA and frida-il2cpp-bridge",
  "keywords": [
    "frida",
    "il2cpp",
    "unity",
    "fallguys",
    "modmenu"
  ],
  "homepage": "https://github.com/repinek/fallguys-frida-modmenu#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/repinek/fallguys-frida-modmenu.git"
  },
  "bugs": {
    "url": "https://github.com/repinek/fallguys-frida-modmenu/issues"
  },
  "license": "GPL-3.0",
  "author": "repinek",
  "main": "src/index.ts",
  "scripts": {
    "prepare": "npm run build:release",
    "build:release": "webpack --env release",
    "build:dev": "webpack --env dev",
    "build:staging": "webpack --env staging",
    "watch": "webpack --watch --env dev",
    "spawn": "frida -U gadget -l dist/agent.js",
    "spawn:release": "npm run build:release && frida -U gadget -l dist/agent.js",
    "spawn:staging": "npm run build:staging && frida -U gadget -l dist/agent.js",
    "spawn:dev": "npm run build:dev && frida -U gadget -l dist/agent.js",
    "lint": "eslint \"src/**\"",
    "prettier": "prettier --write \"src/**\""
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/frida-gum": "^18.7.1",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "frida-compile": "^16.4.1",
    "globals": "^16.5.0",
    "ifdef-loader": "^2.3.2",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.4",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.47.0",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "frida-il2cpp-bridge": "^0.9.1",
    "frida-java-menu": "github:commonuserlol/frida-java-menu"
  },
  "prettier": {
    "tabWidth": 4,
    "arrowParens": "avoid",
    "trailingComma": "none",
    "printWidth": 160
  }
}
